@page "/GetMeeting"
<div class="form-control">
    <h1>Get Meeting</h1>
    <h2>JSON Response</h2>
    <label>@JSONResponse</label>
    <br />
    <p>Status: @numericStatusCode1</p>
    <button @onclick="GetAllMeetings">
        Get All Meetings
    </button>
    <p>Status: @numericStatusCode2</p>
    <button @onclick="(e => GetAMeeting())">
        Get One Meeting
    </button>
</div>